{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "global-require": "off",
        "no-unused-vars": ["error", {"args": "none"}],
        "import/no-dynamic-require": "off"
    }
}